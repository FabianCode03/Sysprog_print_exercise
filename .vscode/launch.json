  "configurations": [
    {
      "name": "Debug myProgram",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/myProgram",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
